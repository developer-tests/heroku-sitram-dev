@charset "utf-8";
/* CSS Document */
* {
  box-sizing: border-box;
}
html {
  scroll-behavior: smooth;
}
.fixed-top {
  position: absolute;
  top: 15px;
}
.carousel-item {
  height: 100vh;
  min-height: 300px;
  background: no-repeat center center scroll;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
  background-size: cover;
}
.carousel-caption {
  bottom: 190px;
}
.carousel-caption h5 {
  font-size: 60px;
  text-align: left;
  letter-spacing: 3px;
  margin-bottom: 50px;
  font-weight: 300;
  margin-left: -25px;
}
.carousel-caption button {
  background-color: #0b7dda;
  border: none;
  border-radius: 5px;
  color: #ffffff;
  float: left;
  font-size: 22px;
  padding: 10px 20px;
}
.navbar-light .navbar-brand {
  color: #fff;
  font-size: 25px;
  text-transform: uppercase;
  font-weight: bold;
  letter-spacing: 2px;
}
.navbar-light .navbar-nav .active > .nav-link, .navbar-light .navbar-nav .nav-link.active, .navbar-light .navbar-nav .nav-link.show, .navbar-light .navbar-nav .show > .nav-link {
  color: #99c2de;
  margin-right: 30px;
}
.navbar-light .navbar-nav .nav-link {
  color: #99c2de;
}
.navbar-toggler {
  background: #fff;
}
.navbar-nav {
  text-align: center;
}
.nav-link {
  padding: .2rem 1rem;
}
.nav-link.active, .nav-link:focus {
  color: #fff;
}
.navbar-toggler {
  padding: 1px 5px;
  font-size: 18px;
  line-height: 0.3;
}
.navbar-light .navbar-nav .nav-link:focus, .navbar-light .navbar-nav .nav-link:hover {
  color: #99c2de;
}
/* ignore the code below */
.link-area {
  position: fixed;
  bottom: 20px;
  left: 20px;
  padding: 15px;
  border-radius: 40px;
  background: tomato;
}
.link-area a {
