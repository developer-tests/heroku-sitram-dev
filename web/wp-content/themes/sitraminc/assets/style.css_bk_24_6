	font-size: 0;
	transition: 0.8s;
}

/* SLIDES */

.section_five_slider .CSSgal .slider > * {
	font-size: 1rem;
	display: inline-block;
	white-space: normal;
	vertical-align: top;
	height: 100%;
	width: 100%;
	background: none 50% no-repeat;
	background-size: cover;
}

/* PREV/NEXT, CONTAINERS & ANCHORS */

.section_five_slider .CSSgal .prevNext {
	position: absolute;
    top: 95%;
    width: 100%;
	height: 0;
}

.section_five_slider .CSSgal .prevNext > div+div {
	visibility: hidden; /* Hide all but first P/N container */
}

.section_five_slider .CSSgal .prevNext a {
	background: transparent;
    font-size: 20px;
    position: absolute;
    -webkit-transition: 0.3s;
    transition: 0.3s;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
    left: 42%;
}
.section_five_slider .CSSgal .prevNext a:hover {
	opacity: 1;
}
.section_five_slider .CSSgal .prevNext a+a {
	left: auto;
	right:42%;
}

/* NAVIGATION */

.section_five_slider .CSSgal .bullets {
	position: absolute;
	bottom: 0;
	padding: 10px 0;
	padding: 10px 0;
	width: 100%;
	text-align: center;
}
.section_five_slider .CSSgal .bullets > a {
	display: inline-block;
	line-height: 30px;
	text-decoration: none;
	text-align: center;
