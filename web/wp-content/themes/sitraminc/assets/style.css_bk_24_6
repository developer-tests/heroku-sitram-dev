/** =======================
 * Contenedor Principal
 ===========================*/

.dropdown_accordion .accordion {
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
}
.dropdown_accordion .accordion .link {
    cursor: pointer;
    display: block;
    padding: 15px 15px 15px 50px;
    color: #4D4D4D;
    font-size: 17px;
    font-weight: 600;
    border: 1px solid #1e75bd;
    position: relative;
    -webkit-transition: all 0.4s ease;
    -o-transition: all 0.4s ease;
    transition: all 0.4s ease;
	margin-top: 20px;
	background-color: #f4f6fc;
	color: #1e75bd;
}
.dropdown_accordion .accordion li:last-child .link {
}
.dropdown_accordion .accordion li i {
    position: absolute;
    top: 20px;
    left: 12px;
    font-size: 20px;
    color: #0b7dda;
    -webkit-transition: all 0.4s ease;
    -o-transition: all 0.4s ease;
    transition: all 0.4s ease;
